//use for getting user input
      Scanner sc = new Scanner(System.in);
      int testCase = sc.nextInt();
      while (testCase > 0) {
         int n = sc.nextInt();
         int[] inputArr1 = new int[n];
         int len = n;
         for (int i = 0; i < n; i++) {
            inputArr1[i] = sc.nextInt();
         }
         testCase--;
      }
      sc.close();
      